    <link rel="stylesheet" href="css/highlight-styles/default.css">
</head>

<body>

    <div class="navbar">
	<div class="navbar-inner">
	    <ul class="nav" style="margin-left:-60px">
		<li class="active"><a href="#">Search</a></li>
		<li><a href="/publish">Publish</a></li>
		<li><a href="/super_demo_slides/index.html">Demo</a></li>
	    </ul>

	    <% if (user) { %>
	    <a class="brand pull-right" href="/u/<%= user.id %>"><%=user.displayName%> Â· <%= userExtra.rep %></a>
	    <%}%>
	    <% if (!user) { %>
	    <a class="brand" href="/auth/google">Log in</a>
	    <%}%>

	</div>
    </div>

    <div id="logo">
	<center>
	<a href="/super_demo_slides/index.html"><img id="logoimg" src="/img/quadcode.png"></a>
	</center>
    </div>

    <div>

	<header>
	    <div id="search">
		<label>Search:</label>
		<div class="search-bar">
		    <% if (query) { %>
		    <input id="search-input" type="text" value="<%= query %>" autofocus="autofocus">
		    <% } %>
		    <% if (!query) { %>
		    <input id="search-input" type="text" placeholder="Name of algorithm or code" autofocus="autofocus">
		    <% } %>

		</div>
		<button id="find-button" class="button blue">Find Code</button>
	    </div>



	    <ul id="lang-and-code-results" class="inline-block hidden">
		<li>
	    <div id="language-results">
		<!--
		<div class="lang-item lang-active">
		    <span>Haskell <span class="lang-number">3</span></span>
		</div>
		<hr>
		<div class="lang-item">
		    <span>Java <span class="lang-number">69</span></span>
		</div>
		<hr>
		<div class="lang-item">
		    <span>Python <span class="lang-number">103</span></span>
		</div>
		-->
	    </div>
		</li>
		<li>
		    <div id="publish-bar">+ Add your own implementation</div>
		    <div id="search-results">


			<div class="result">
			    <ul class="inline-block">
				<li>

				    <div class="avatars">
